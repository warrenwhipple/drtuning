{
  "name": "drtuning",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "^3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
